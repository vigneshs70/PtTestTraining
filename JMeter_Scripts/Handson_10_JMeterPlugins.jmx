          <stringProp name="BeanShellSampler.query">import java.util.Random;

String ProductList = &quot;MobilePhone,Laptop,Television,Refrigerator,AirCooler &quot;; // Your input array of strings
String[] sSearchStrings = ProductList.split(&quot;,&quot;); // Split by space

// Remove leading and trailing spaces from each string
for (int i = 0; i &lt; sSearchStrings.length; i++) {
    sSearchStrings[i] = sSearchStrings[i].replaceAll(&quot;,&quot;, &quot;&quot;).replaceAll(&quot; &quot;, &quot;&quot;).replaceAll(&quot;AirCooler&quot;, &quot;AirConditioner&quot;).toUpperCase();
    log.info(&quot;String&quot; +&quot; &quot; + i + &quot; :&quot;+sSearchStrings[i]);  
}

Random rand = new Random();

int randomIndex = rand.nextInt(5);

String randomValue = sSearchStrings[randomIndex];


log.info(&quot;Random Product : &quot;+ randomValue);
log.info(&quot;${__javaScript(&quot;This is My Test String&quot;.split(&apos; &apos;)[2],)}&quot;);
log.info(&quot;Encoded Value : &quot;+&quot;${Encode}&quot;);
log.info(&quot;Decoded Value : &quot;+&quot;${__base64Decode(${Encode},)}&quot;);


</stringProp>
